{
  "name": "AutonomousSport",
  "version": "0.0.1",
  "private": true,
  "rnpm": {
    "assets": [
      "./src/assets/fonts/Roboto"
    ]
  },
  "scripts": {
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "android:clean": "cd android && ./gradlew clean",
    "buildStagingRelease": "rm -rf android/app/src/main/res/drawable-xxxhdpi android/app/src/main/res/drawable-xxhdpi android/app/src/main/res/drawable-xhdpi android/app/src/main/res/drawable-mdpi android/app/src/main/res/drawable-hdpi && cd android && ENVFILE=.env ./gradlew assembleStagingRelease",
    "buildRelease": "rm -rf android/app/src/main/res/drawable-xxxhdpi android/app/src/main/res/drawable-xxhdpi android/app/src/main/res/drawable-xhdpi android/app/src/main/res/drawable-mdpi android/app/src/main/res/drawable-hdpi && cd android && ENVFILE=.env.production ./gradlew assembleProductionRelease",
    "buildDebug": "mkdir -p android/app/src/main/assets && rm -rf android/app/build && ENVFILE=.env react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && cd android && ./gradlew assembleStagingDebug",
    "test": "jest",
    "flow": "flow",
    "flow start": "flow start",
    "flow stop": "flow stop",
    "android": "react-native run-android --variant=stagingDebug",
    "androidProduction": "react-native run-android --variant=productionDebug",
    "androidRelease": "react-native run-android --variant=stagingRelease",
    "release": "react-native run-android --variant=productionRelease",
    "ios": "react-native run-ios",
    "reload": "adb shell input keyevent 82",
    "log:android": "react-native log-android",
    "log:ios": "react-native log-ios"
  },
  "dependencies": {
    "convert-string": "^0.1.0",
    "opentok-react-native": "^0.10.3",
    "prop-types": "^15.6.2",
    "pubnub": "^4.23.0",
    "react": "16.8.3",
    "react-native": "0.59.9",
    "react-native-animatable": "^1.3.0",
    "react-native-ble-manager": "6.6.4",
    "react-native-config": "^0.11.5",
    "react-native-device-detection": "^0.2.0",
    "react-native-easy-toast": "^1.1.0",
    "react-native-elements": "^1.1.0",
    "react-native-fast-image": "^5.2.0",
    "react-native-game-engine": "^0.11.5",
    "react-native-image-pan-zoom": "^2.1.9",
    "react-native-image-progress": "^1.1.1",
    "react-native-popup-dialog": "^0.15.1",
    "react-native-radial-gradient": "^1.0.5",
    "react-native-segmented-control-tab": "^3.3.1",
    "react-native-size-matters": "^0.1.7",
    "react-native-snap-carousel": "^3.7.2",
    "react-native-sound-player": "^0.5.3",
    "react-native-svg": "^7.0.3",
    "react-native-svg-uri": "^1.2.3",
    "react-native-vector-icons": "^6.5.0",
    "react-navigation": "^2.7.0",
    "react-navigation-redux-helpers": "^2.0.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^24.8.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "eslint": "^5.1.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-react-native": "^3.2.1",
    "flow-bin": "^0.92.0",
    "jest": "^24.8.0",
    "metro-react-native-babel-preset": "^0.54.1",
    "prettier-eslint": "^8.8.1",
    "react-test-renderer": "16.8.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
